{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "think-cell data for PowerPoint",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"template": {
				"type": "string",
				"pattern": "^[^\u0000-\u001F\u007F]+$"
			},
			"data": {
				"type": "array",
				"items": {
					"type": "object",
					"properties": {
						"name": {
							"type": "string",
							"pattern": "^[^\u0000-\u001F\u007F]+$"
						},
						"table": {
							"type": "array",
							"items": {
								"type": "array",
								"items": {
									"oneOf": [
										{
											"type": "null"
										},
										{
											"type": "object",
											"properties": {
												"string": {
													"type": "string"
												},
												"number": {
													"type": "number"
												},
												"percentage": {
													"type": "number"
												},
												"date": {
													"type": "string",
													"pattern": "^[0-9]{4}((0[1-9]|1[0-2])((0[1-9]|[1-2][0-9]|3[0-1])(T([0-1][0-9]|2[0-3])([0-5][0-9]([0-5][0-9])?)?)?)?|-(0[1-9]|1[0-2])(-(0[1-9]|[1-2][0-9]|3[0-1])(T([0-1][0-9]|2[0-3])(:[0-5][0-9](:[0-5][0-9])?)?)?)?)?Z?$"
												},
												"fill": {
													"type": "string",
													"pattern": "^(#[0-9A-Fa-f]{6}|rgb\\((0*(2(5[0-5]|[0-4][0-9])|1[0-9]{2}|[0-9]{1,2}),){2}0*(2(5[0-5]|[0-4][0-9])|1[0-9]{2}|[0-9]{1,2})\\))$"
												}
											},
											"oneOf": [
												{
													"required": [
														"string"
													]
												},
												{
													"required": [
														"number"
													]
												},
												{
													"required": [
														"percentage"
													]
												},
												{
													"required": [
														"date"
													]
												}
											]
										}
									]
								}
							}
						}
					},
					"required": [
						"name",
						"table"
					]
				}
			}
		},
		"required": [
			"template",
			"data"
		]
	}
}